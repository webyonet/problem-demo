{
  "presets": [
   [
     "@babel/preset-env",
     {
       "useBuiltIns": "usage",
       "corejs": "3.26.1"
     }
   ],
    "@babel/preset-react",
    "@babel/preset-flow"
  ],
  "plugins": [
    "jsx-control-statements",
    [
      "@babel/plugin-proposal-decorators",
      {
        "version": "legacy"
      }
    ]
  ],
  "env": {
    "production": {
      "plugins": [
        "transform-remove-console",
        "transform-remove-debugger"
      ]
    }
  }
}
